{
    "name": "random-leetcode-algorithms",
    "version": "1.0.6",
    "description": "Sample of Leetcode algorithms I solved at one time or another using JS",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "files": [
        "dist"
    ],
    "scripts": {
        "build": "tsc",
        "watch-ts": "tsc -w",
        "eslint": "eslint --fix --format table --ext .ts --ext .json --ext .html 'src/**'",
        "test": "mocha -r ts-node/register test/**/*.test.ts",
        "coverage": "nyc npm test"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/montrealist/2020-04-17-personal-npm-package.git"
    },
    "keywords": [
        "leetcode",
        "algorithms"
    ],
    "author": "montrealist",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/montrealist/2020-04-17-personal-npm-package/issues"
    },
    "homepage": "https://github.com/montrealist/2020-04-17-personal-npm-package#readme",
    "devDependencies": {
        "@types/chai": "^4.2.11",
        "@types/mocha": "^7.0.2",
        "@types/node": "^13.11.1",
        "@typescript-eslint/eslint-plugin": "^2.28.0",
        "@typescript-eslint/parser": "^2.28.0",
        "chai": "^4.2.0",
        "cross-env": "^7.0.2",
        "eslint": "^6.8.0",
        "eslint-config-prettier": "^6.10.1",
        "eslint-plugin-prettier": "^3.1.3",
        "mocha": "^7.1.1",
        "nyc": "^15.0.1",
        "prettier": "^2.0.4",
        "ts-node": "^8.8.2",
        "typescript": "^3.8.3"
    },
    "eslintConfig": {
        "parser": "@typescript-eslint/parser",
        "extends": [
            "plugin:@typescript-eslint/recommended",
            "plugin:prettier/recommended",
            "prettier",
            "prettier/@typescript-eslint"
        ]
    },
    "prettier": {
        "endOfLine": "lf",
        "printWidth": 160,
        "singleQuote": true,
        "semi": true,
        "trailingComma": "es5",
        "tabWidth": 4,
        "useTabs": false
    }
}
